[colors.bright]
black = "#545454"
blue = "#94bff3"
cyan = "#93e0e3"
green = "#8ae234"
magenta = "#b294bb"
red = "#fc5454"
white = "#ffffff"
yellow = "#fce94f"

[colors.normal]
black = "#3f3f3f"
blue = "#0698ca"
cyan = "#94bff3"
green = "#1e8a1f"
magenta = "#d244ef"
red = "#cc0000"
white = "#dcdccc"
yellow = "#c4a000"

[colors.primary]
background = "#111111"
foreground = "#F9F9F9"

[font]
size = 18.0

[font.bold]
family = "JetBrainsMonoNLNerdFont"
style = "Bold"

[font.bold_italic]
family = "JetBrainsMonoNLNerdFont"
style = "Bold Italic"

[font.italic]
family = "JetBrainsMonoNLNerdFont"
style = "Italic"

[font.normal]
family = "JetBrainsMonoNLNerdFont"
style = "Regular"

[[keyboard.bindings]]
action = "ToggleFullscreen"
key = "F11"

[scrolling]
history = 50000

[window]
opacity = 0.9
